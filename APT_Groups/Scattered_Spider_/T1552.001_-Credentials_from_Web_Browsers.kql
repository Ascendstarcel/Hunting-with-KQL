// T1552.001 - Credentials from Web Browsers
// Detect access or export of browser stored passwords
DeviceProcessEvents
| where FileName in ("chrome.exe", "firefox.exe", "msedge.exe")
| where ProcessCommandLine contains_any ("passwords", "signons", "credentials")
| project Timestamp, DeviceName, ProcessCommandLine, InitiatingProcessAccountName
| order by Timestamp desc
